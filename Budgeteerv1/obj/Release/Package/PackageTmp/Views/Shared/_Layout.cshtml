<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    @Styles.Render("~/bundles/css")
    @Scripts.Render("~/bundles/modernizr")
    
    <!-- Google Font: Open Sans -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,800,800italic">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Oswald:400,300,700">

    <!-- Font Awesome CSS -->
    <link rel="stylesheet" href="/Content/font-awesome.min.css">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

    @RenderSection("css", required: false)

    <!-- App CSS -->
    <link rel="stylesheet" href="/Content/mvpready-admin.css">
    <!-- <link rel="stylesheet" href="./css/custom.css"> -->

</head>
<body>  
    <div id="wrapper">
        <!--Nav 1-->
        <header class="navbar" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <i class="fa fa-cog"></i>
                    </button>
                    <a href="@Url.Action("Index","Dashboard")" class="navbar-brand navbar-brand-img">
                        <img src="~/Images/financialportallogo.png" alt="MVP Ready">
                    </a>
                </div> <!-- /.navbar-header -->
                <nav class="collapse navbar-collapse" role="navigation">
                    <ul class="nav navbar-nav navbar-left">


                        @Html.Action("DashNotification", "Dashboard")

                    </ul>

                    <ul class="nav navbar-nav navbar-right">
                        
                        <li class="dropdown navbar-profile">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:;">
                                @Html.Action("ProfileImage","Dashboard")
                            </a>
                            <ul class="dropdown-menu" role="menu">
                                <li>
                                    <a href="@Url.Action("ProfileSettings","Dashboard")">
                                        <i class="fa fa-user"></i>
                                        &nbsp;&nbsp;My Profile
                                    </a>
                                </li>
                                
                                <li class="divider"></li>
                                @Html.Partial("_DashboardLoginPartial")
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div> <!-- /.container -->
        </header>
       
        <br /><br /><br />
        <div class="content">
            @RenderBody()
            <hr />
        </div>
    </div>
    <footer class="footer">
        <div class="container">
            <p class="pull-left">Copyright &copy; 2013-15 Jumpstart Themes.</p>
        </div>
        
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <!-- Plugin JS -->
    <script src="/Scripts/excanvas.min.js"></script>
    <script src="/Scripts/jquery.flot.js"></script>
    <script src="/Scripts/jquery.flot.pie.js"></script>
    <script src="/Scripts/jquery.flot.resize.js"></script>
    <script src="/Scripts/jquery.flot.time.js"></script>
    <script src="/Scripts/jquery.flot.tooltip.js"></script>
    <script src="~/Scripts/jquery.flot.categories.js"></script>
    <script src="~/Scripts/jquery.flot.orderBars.js"></script>
    <script src="~/Scripts/jquery.flot.stack.js"></script>
    <script src="/Scripts/jquery.dataTables.min.js"></script>
    <script src="/Scripts/jquery.waypoints.min.js"></script>
    <script src="/Scripts/jquery.counterup.min.js"></script>
    <script src="/Scripts/jqmeter.min.js"></script>

    
    @RenderSection("page",required: false)
        
    <!-- App JS -->
    <script src="/Scripts/mvpready-core.js"></script>
    <script src="/Scripts/mvpready-helpers.js"></script>
    <script src="/Scripts/mvpready-admin.js"></script>
    
</body>
</html>
